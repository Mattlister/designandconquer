// Now this function is dynamic and can be used for all markers
function addmarker(col) {
        var image = col[1].url;
        
        var marker = new google.maps.Marker({
        position: new google.maps.LatLng(col[0].lat, col[0].lng),
                title: col[2].title,
        draggable: true,
        map: map0,
        icon: image
    });
    
}
