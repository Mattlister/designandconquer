function addMarker(activity) {
    for (let i = 0; i < activity.length; i++) {
      let marker = new google.maps.Marker({
        position: (activity[i].lat, activity[i].lng),
        title: activity[i].name,
        draggable: true,
        map: 'map' + i,
        icon: activity[i].icon
      });
    }
  }
